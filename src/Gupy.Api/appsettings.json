{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "PhotoSettings": {
    "MaxBytes": 10485760,
    "AcceptedFileTypes": [
      ".jpg",
      ".jpeg",
      ".png",
      ".svg"
    ]
  },
  "Placeholder": {
    "Url": "https://via.placeholder.com"
  },
  "PhotoProcessorSettings": {
    "Height": 300,
    "Width": 400
