  "AzureSettings": {
    "StorageUrl": "https://twopizza.blob.core.windows.net",
    "ContainerName": "images"
  },
  "Auth0": {
    "Domain": "https://neistow.eu.auth0.com",
    "Audience": "https://twopizzaproject.com"
  },
  "Data": {
    "ServerVersion": "8.0.19-mysql",
    "CommandTimeout": "600"
  },
  "HostName": "https://localhost:5001",
  "AllowedHosts": "*"
